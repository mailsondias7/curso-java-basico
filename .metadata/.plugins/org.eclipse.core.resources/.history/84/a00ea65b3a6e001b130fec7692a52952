package com.teste.cursojava.aulas;

public class LoopFor {

	public static void main(String[] args) {
		//Incremento
		for (int i = 0; i < 5; i++) {
			System.out.println("i tem valor de: " + i);
		}
		
		//Decremento
		for (int j = 6; j > 0; j--) {
			System.out.println("i tem valor de: " + j);
		}

	}

}
