package com.teste.cursojava.aula01;

public class VariaveisPontoFlutuantes {

	public static void main(String[] args) {
		
		double valorPassagem = 4.20;
		
		float ingressoShow = 15.75f;
		
		System.out.println("Valor da Passagem: " + valorPassagem);
		System.out.println("Valor Ingresso: " + ingressoShow);

	}

}
